{
  "expo": {
    "name": "Palindrome",
    "slug": "Palindrome",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bulldog.png",
    "scheme": "palindrome",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gammagames.palindrome"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bulldog.png",
        "backgroundImage": "./assets/images/bg.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.gammagames.palindrome",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/bulldog.png",
      "backgroundColor": "#000D3F",
      "themeColor": "#0060FF"
    },
    "splash": {
      "image": "./assets/images/bg.png",
      "resizeMode": "contain",
      "backgroundColor": "#000D3F"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/bg.png",
          "resizeMode": "contain",
          "backgroundColor": "#000D3F",
          "dark": {
            "backgroundColor": "#000D3F"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}