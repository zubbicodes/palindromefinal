{
  "expo": {
    "name": "Palindrome",
    "slug": "Palindrome",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bulldog.png",
    "scheme": "palindrome",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gammagames.palindrome"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bulldog.png",
        "backgroundImage": "./assets/images/bulldog.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.gammagames.palindrome"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/bulldog.png",
      "backgroundColor": "#000D3F",
      "themeColor": "#0060FF"
    },
    "splash": {
      "image": "./assets/images/bulldog.png",
      "resizeMode": "contain",
      "backgroundColor": "#000D3F"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/bulldog.png",
          "resizeMode": "contain",
          "backgroundColor": "#000D3F",
          "dark": {
            "backgroundColor": "#000D3F"
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": true,
            "packagingOptions": {
              "pickFirst": [
                "lib/**/libc++_shared.so"
              ]
            },
            "kotlinVersion": "2.1.20",
            "extraMavenRepos": []
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "18ac16ae-ca64-43ff-a7ac-beaa9dd69c6a"
      }
    }
  }
}
